#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main()

{
	int a,b,s=0,i=0;

	cout<< "Ingrese el primer valor: "<<endl;
	cin>> a;
	cout<< "Ingrese el segundo valor: "<<endl;
	cin>> b;

	do{
		s=s+a;
		i++;
	}while(i<b);

	cout<<"la multiplicacion es igual a: "<<s<<endl;

	getch();
